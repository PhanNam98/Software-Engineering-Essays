<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <link href="~/coolsdmin/css/font-face.css" rel="stylesheet" media="all">
    <link href="~/coolsdmin/vendor/font-awesome-4.7/css/font-awesome.min.css" rel="stylesheet" media="all">
    <link href="~/coolsdmin/vendor/font-awesome-5/css/fontawesome-all.min.css" rel="stylesheet" media="all">
    <link href="~/coolsdmin/vendor/mdi-font/css/material-design-iconic-font.min.css" rel="stylesheet" media="all">
    <!-- Bootstrap CSS-->
    <link href="~/coolsdmin/vendor/bootstrap-4.1/bootstrap.min.css" rel="stylesheet" media="all">

    <!-- Vendor CSS-->
    <link href="~/coolsdmin/vendor/animsition/animsition.min.css" rel="stylesheet" media="all">
    <link href="~/coolsdmin/vendor/bootstrap-progressbar/bootstrap-progressbar-3.3.4.min.css" rel="stylesheet" media="all">
    <link href="~/coolsdmin/vendor/wow/animate.css" rel="stylesheet" media="all">
    <link href="~/coolsdmin/vendor/css-hamburgers/hamburgers.min.css" rel="stylesheet" media="all">
    <link href="~/coolsdmin/vendor/slick/slick.css" rel="stylesheet" media="all">
    <link href="~/coolsdmin/vendor/select2/select2.min.css" rel="stylesheet" media="all">
    <link href="~/coolsdmin/vendor/perfect-scrollbar/perfect-scrollbar.css" rel="stylesheet" media="all">
    <!-- Main CSS-->
    <link href="~/coolsdmin/css/theme.css" rel="stylesheet" media="all">
 
</head>

<body>

    <div class="page-wrapper">
        <div class="page-content--bge5">
            
                <div class="login-wrap">
                    <div class="login-content">

                            @RenderBody()
                    </div>
                </div>
            
        </div>
    </div>

        <!-- Jquery JS-->
        <script src="~/coolsdmin/vendor/jquery-3.2.1.min.js"></script>
        <!-- Bootstrap JS-->
        <script src="vendor/bootstrap-4.1/popper.min.js"></script>
        <script src="vendor/bootstrap-4.1/bootstrap.min.js"></script>
        <!-- Vendor JS       -->
        <script src="~/coolsdmin/vendor/slick/slick.min.js"></script>
        <script src="~/coolsdmin/vendor/wow/wow.min.js"></script>
        <script src="~/coolsdmin/vendor/animsition/animsition.min.js"></script>
        <script src="~/coolsdmin/vendor/bootstrap-progressbar/bootstrap-progressbar.min.js"></script>
        <script src="~/coolsdmin/vendor/counter-up/jquery.waypoints.min.js"></script>
        <script src="~/coolsdmin/vendor/counter-up/jquery.counterup.min.js"></script>
        <script src="~/coolsdmin/vendor/circle-progress/circle-progress.min.js"></script>
        <script src="~/coolsdmin/vendor/perfect-scrollbar/perfect-scrollbar.js"></script>
        <script src="~/coolsdmin/vendor/chartjs/Chart.bundle.min.js"></script>
        <script src="~/coolsdmin/vendor/select2/select2.min.js"></script>
        <!-- Main JS-->
        <script src="js/main.js"></script>
        @RenderSection("Scripts", required: false)
</body>
</html>
