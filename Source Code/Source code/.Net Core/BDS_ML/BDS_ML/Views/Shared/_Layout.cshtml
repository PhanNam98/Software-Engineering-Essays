    <!-- Header -->
    <header id="header">
        <!-- Nav -->
        <div id="nav">
            <!-- Main Nav -->
            <div id="nav-fixed">
                <div class="container">
                    <!-- logo -->
                    <div class="nav-logo">
                        <a asp-area="" asp-controller="Home" asp-action="Index"><img src="~/logo/logo.png" alt="WebMag" height="64"></a>
                    </div>
                    <!-- /logo -->
                    <!-- nav -->
                    <ul class="nav-menu nav navbar-nav">
                        @*<li><a href="#">News</a></li>*@
                        @*<li><a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a></li>*@
                        <li><a asp-area="" asp-controller="Post" asp-action="Create">Đăng bài</a></li>
                        @*<li class="cat-2"><a class="nav-link text-dark" asp-area="" asp-controller="Post" asp-action="Search">Lọc bài đăng</a></li>*@
                        <li class="cat-1"><a asp-action="SearchByPostType" asp-controller="Post" asp-route-id="0">Cần Mua/Cần Thuê</a></li>
                        <li class="cat-2"><a asp-action="SearchByPostType" asp-controller="Post" asp-route-id="1">Cần Bán/Cho Thuê</a></li>
                        <li>
                            <a asp-area="" asp-controller="Predict" asp-action="Index">
                                Định giá<span style="position: absolute; top: 50%; left: 50%; margin-top: -30px; margin-left: 10px;
                                                padding-right: 0.6em; padding-left: 0.6em; border-radius: 10rem; color: #fff; background-color: #f86c6b; font-size: 0.6em;">
                                    beta
                                </span>
                            </a>
                        </li>
                        <partial name="_LoginPartial" />
                    </ul>
                    <!-- /nav -->
                    <!-- search & aside toggle -->
                    <div class="nav-btns">
                        <a class="nav-link text-dark" asp-area="" asp-controller="Post" asp-action="Search"><i class="fa fa-search"></i> Tìm Kiếm Ngay!</a>
                        <button class="aside-btn"><i class="fa fa-bars"></i></button>
                        @*<button class="search-btn"><i class="fa fa-search"></i></button>
                            <div class="search-form">
                                <input class="search-input" type="text" name="search" placeholder="Enter Your Search ...">
                                <button class="search-close"><i class="fa fa-times"></i></button>
                            </div>*@
                    </div>
                    <!-- /search & aside toggle -->
                </div>
            </div>
            <!-- /Main Nav -->
            <!-- Aside Nav -->
            <div id="nav-aside">
                <!-- nav -->
                <div class="section-row">
                    <ul class="nav-aside-menu">
